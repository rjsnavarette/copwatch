%aside#leftsidebar.sidebar
  .tab-content
    #dashboard.tab-pane.stretchRight.active
      .menu
        .slimScrollDiv{:style => "position: relative; overflow: hidden; width: auto; height: calc(100vh - 60px);"}
          %ul.list{:style => "overflow: hidden; width: auto; height: calc(100vh - 60px);"}
            %li
              .user-info
                .image
                  %a.waves-effect.waves-block{:href => ""}
                    %img{:alt => "Admin", :src => ""}
                .detail
                  %h4= admin_data('name')
                  %small
            %li.header MAIN
            %li.active.open
              %a.menu-toggle.toggled.waves-effect.waves-block{:href => "javascript:void(0);"}
                %i.zmdi.zmdi-accounts-outline
                %span Users
              %ul.ml-menu{:style => "display: block;"}
                %li.active
                  %a.toggled.waves-effect.waves-block{:href => "students.html"} All Users
          .slimScrollBar{:style => "background: rgba(0, 0, 0, 0.2); width: 2px; position: absolute; top: 0px; opacity: 0.4; display: block; border-radius: 3px; z-index: 99; left: 1px; height: 644.019px;"}
          .slimScrollRail{:style => "width: 2px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 0px; background: rgb(51, 51, 51); opacity: 0.2; z-index: 90; left: 1px;"}
    #user.tab-pane.stretchLeft
      .menu
        .slimScrollDiv{:style => "position: relative; overflow: hidden; width: auto; height: calc(100vh - 60px);"}
          %ul.list{:style => "overflow: hidden; width: auto; height: calc(100vh - 60px);"}
            %li
              .user-info.m-b-20.p-b-15
                .image
                  %a.waves-effect.waves-block{:href => "profile.html"}
                    %img{:alt => "User", :src => ""}
                .detail
                  %h4 Pro. Charlotte
                  %small Design Faculty
                .row
                  .col-12
                    %a.waves-effect.waves-block{:href => "javascript:void(0);", :title => "facebook"}
                      %i.zmdi.zmdi-facebook
                    %a.waves-effect.waves-block{:href => "javascript:void(0);", :title => "twitter"}
                      %i.zmdi.zmdi-twitter
                    %a.waves-effect.waves-block{:href => "javascript:void(0);", :title => "instagram"}
                      %i.zmdi.zmdi-instagram
                  .col-4.p-r-0
                    %h5.m-b-5 13
                    %small Exp
                  .col-4
                    %h5.m-b-5 33
                    %small Awards
                  .col-4.p-l-0
                    %h5.m-b-5 237
                    %small Class
            %li
              %small.text-muted Location:
              %p 795 Folsom Ave, Suite 600 San Francisco, CADGE 94107
              %hr/
              %small.text-muted Email address:
              %p Charlotte@example.com
              %hr/
              %small.text-muted Phone:
              %p + 202-555-0191
              %hr/
              %ul.list-unstyled
                %li
                  %div Photoshop
                  .progress.m-b-20
                    .progress-bar.l-blue{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "89", :role => "progressbar", :style => "width: 89%"}
                      %span.sr-only 89% Complete
                %li
                  %div Illustrator
                  .progress.m-b-20
                    .progress-bar.l-amber{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "56", :role => "progressbar", :style => "width: 56%"}
                      %span.sr-only 56% Complete
                %li
                  %div Art & Design
                  .progress.m-b-20
                    .progress-bar.l-green{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "78", :role => "progressbar", :style => "width: 78%"}
                      %span.sr-only 78% Complete
                %li
                  %div HTML
                  .progress.m-b-20
                    .progress-bar.l-blush{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "43", :role => "progressbar", :style => "width: 56%"}
                      %span.sr-only 56% Complete
                %li
                  %div CSS
                  .progress.m-b-20
                    .progress-bar.l-parpl{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "43", :role => "progressbar", :style => "width: 50%"}
                      %span.sr-only 50% Complete
          .slimScrollBar{:style => "background: rgba(0, 0, 0, 0.2); width: 2px; position: absolute; top: 0px; opacity: 0.4; display: block; border-radius: 3px; z-index: 99; left: 1px;"}
          .slimScrollRail{:style => "width: 2px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 0px; background: rgb(51, 51, 51); opacity: 0.2; z-index: 90; left: 1px;"}